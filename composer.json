{
    "name": "shipmonk/packing",
    "description": "Testing day application stub",
    "require": {
        "ext-pdo": "*",
        "ext-json": "*",
        "php": ">=8.4",
        "psr/http-message": "2.*",
        "guzzlehttp/psr7": "2.*",
        "doctrine/orm": "3.*",
        "symfony/cache": "7.*",
        "guzzlehttp/guzzle": "^7.9",
        "slim/slim": "^4.15",
        "php-di/php-di": "^7.1",
        "doctrine/migrations": "^3.9",
        "symfony/dotenv": "^8.0",
        "doctrine/doctrine-fixtures-bundle": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12.5"
    }
}
